{
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4,
    "bracketSpacing": true,
    "semi": false,
    "endOfLine": "auto",
    "arrowParens": "avoid",
    "importOrderSeparation": true,
    "importOrderSpecifiers": true,
    "plugins": [
        "prettier-plugin-tailwindcss",
        "@trivago/prettier-plugin-sort-imports"
    ]
}
